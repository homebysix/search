---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ site.title | xml_escape }}</title>
    <description>{{ site.description | xml_escape }}</description>
    <link>{{ site.url }}</link>
    <atom:link href="{{ site.url }}/feed.xml" rel="self" type="application/rss+xml" />
    {% assign recipes = site.data.index %}
    {% for recipe_id in recipes.identifiers %}
    {% assign recipe = recipes[recipe_id] %}
      <item>
        <title>{{ recipe.name | xml_escape }}</title>
        <description>
          {{ recipe.description | xml_escape }}
          {{ recipe.repo | xml_escape }}
          {{ recipe.path | xml_escape }}
          {{ recipe.parent | xml_escape }}
          {{ recipe.shortname | xml_escape }}
          {{ recipe.inferred_type | xml_escape }}
          {% if recipe.munki_display_name %}{{ recipe.munki_display_name | xml_escape }}{% endif %}
          {% if recipe.munki_description %}{{ recipe.munki_description | xml_escape }}{% endif %}
        </description>
        <!-- <pubDate>{{ recipe.updated_at | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate> -->
        <link>https://github.com/{{ recipe.repo }}/{{ recipe.path }}</link>
        <guid isPermaLink="true">https://github.com/{{ recipe.repo }}/{{ recipe.path }}</guid>
      </item>
    {% endfor %}
  </channel>
</rss>
